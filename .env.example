ADMIN_ROUTE_PREFIX=dash
ADMIN_HTTPS=false
ADMIN_ASSETS_SERVER=

# Translation Settings
TRANSLATION_PROVIDER=openai
TRANSLATION_JOB_LIMIT=50
AUTO_TRANSLATION_ENABLED=false

# OpenAI Settings
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4-turbo
