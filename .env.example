MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Set when you have CDN for asset
ASSET_URL=

VITE_APP_NAME="${APP_NAME}"
VITE_DEV_SERVER_URL1=http://host.docker.internal:5177
VITE_SSL_KEY_PATH=
VITE_SSL_CERT_PATH=

# Debugbar
DEBUGBAR_REMOTE_SITES_PATH=
