{
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite --port 5177",
        "dev:default": "vite --config themes/default/vite.config.js",
        "build:default": "vite build --config themes/default/vite.config.js",
        "build:page-builder": "vite build --config vite.pagebuilder.config.js",
        "dev:page-builder": "vite --config vite.pagebuilder.config.js",
        "build:all": "npm run build && npm run build:page-builder",
        "postbuild:page-builder": "cp public/build/page-builder/.vite/manifest.json public/build/page-builder/manifest.json",
        "dev:june": "vite --config themes/june/vite.config.js",
        "build:june": "vite build --config themes/june/vite.config.js"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "@tailwindcss/vite": "^4.0.0",
