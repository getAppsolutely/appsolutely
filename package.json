        "autoprefixer": "^10.4.19",
        "axios": "^1.7.4",
        "bootstrap": "^5.3.0",
        "concurrently": "^9.0.1",
        "jquery": "^3.7.0",
        "laravel-vite-plugin": "^1.2.0",
        "popper.js": "^1.16.0",
        "postcss": "^8.4.38",
        "postcss-import": "^16.1.0",
        "sass": "^1.78.0",
        "sass-loader": "^16.0.0",
        "tailwindcss": "^3.4.3",
        "vite": "^6.3.5",
        "vue": "^3.5.16"
    },
    "dependencies": {
        "@headlessui/vue": "^1.7.23",
